                };
                if (_aimPlay) then {
                _nearUnits = nearestObjects [player, ["CAManBase"], _range] select {(side _x) == _side && isPlayer _x and alive _x and _x != player};
                };
                if (_aimAI) then {
                _nearUnits = nearestObjects [player, ["CAManBase"], _range] select {(side _x) == _side && !(isPlayer _x) and alive _x and _x != player};
                };		
                };
