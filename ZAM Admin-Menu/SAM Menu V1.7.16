                    case _aimCiv:{Civilian}
                };
                _nearUnits = nearestObjects [player, ["CAManBase"], _range] select {(side _x) == east and alive _x and alive _x and _x != player};
                if (_aimAll) then {
                if (_aimBoth) then {
